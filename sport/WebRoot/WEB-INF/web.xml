<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginAction</servlet-name>
    <servlet-class>com.sixth.sport.action.LoginAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SystemAction</servlet-name>
    <servlet-class>com.sixth.sport.action.SystemAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserAction</servlet-name>
    <servlet-class>com.sixth.sport.action.UserAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ClassAction</servlet-name>
    <servlet-class>com.sixth.sport.action.ClassAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TrainAction</servlet-name>
    <servlet-class>com.sixth.sport.action.TrainAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Guild</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.sixth.sport.action.Guild</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GuildAction</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.sixth.sport.action.GuildAction</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>LoginAction</servlet-name>
    <url-pattern>/loginAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SystemAction</servlet-name>
    <url-pattern>/systemAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserAction</servlet-name>
    <url-pattern>/userAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ClassAction</servlet-name>
    <url-pattern>/classAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TrainAction</servlet-name>
    <url-pattern>/trainAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Guild</servlet-name>
    <url-pattern>/Guild</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GuildAction</servlet-name>
    <url-pattern>/guildAction</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
